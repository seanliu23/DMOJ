import java.io.*;
import java.util.*;

public class ModInverse{
  public static void main( String[] args){
    Scanner in = new Scanner(System.in);
    int x = in.nextInt();
    int m = in.nextInt();
    
    for (int n = 0; n < m; n++){
      if ((x*n)%m == 1){
        System.out.println(n);
        return;
      }
    }
    System.out.println("No such integer exists.");
  }
}
