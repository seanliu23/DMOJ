import java.util.*;
import java.io.*;

public class Shift {
  public static void main( String[] args) throws Exception {
    Scanner input = new Scanner(System.in);
    
    int N = input.nextInt();
    int K = input.nextInt();
    int x = N;
    
    for ( int i = 0; i < K; i++) {
      x = x*10 + N;
    }
    System.out.println( x );
  }
}
