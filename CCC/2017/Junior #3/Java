import java.util.*;
import java.io.*;

public class ExactlyElectrical {
  public static void main( String[] args ) throws Exception {
    Scanner sc = new Scanner(System.in);
    
    int a = sc.nextInt();
    int b = sc.nextInt();
    int c = sc.nextInt();
    int d = sc.nextInt();
    int t = sc.nextInt();
    
    int dist = Math.abs(a - c) + Math.abs(b - d);
    int x = t - dist;
    
    if ( x >= 0 && x % 2 == 0) {
      System.out.println( "Y" );
    }
    else System.out.println ( "N" );
  }
}
